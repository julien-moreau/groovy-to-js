{
    "name": "groovy-to-js",
    "version": "0.0.1",
    "devDependencies": {
        "@types/mocha": "2.2.43",
        "@types/node": "8.0.19",
        "mocha": "3.5.3",
        "typescript": "2.5.2",
        "assert": "1.4.1",
        "nyc": "11.2.1"
    },
    "dependencies": {
        "js-beautify": "1.6.14"
    },
    "scripts": {
